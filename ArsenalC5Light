{ 
	_box = _x;
	[_box, true, false] call ace_arsenal_fnc_removeVirtualItems;
	switch (_role) do {
default {
	[_box, _genericGear + _genericWeapons + _genericAmmo + _genericAttachmentsAndScopes + _genericItems + [

			"SmokeShell"
			
		],false] call ace_arsenal_fnc_initBox;
	};

